<?xml version="1.0"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">
	<sys:String x:Key="LOC_AU_SuppressNotification">Update-Benachrichtigung unterdrücken</sys:String>
    <sys:String x:Key="LOC_AU_AutoUpdate">Updates automatisch herunterladen und die Installation für den nächsten Neustart planen</sys:String>
	<sys:String x:Key="LOC_AU_Major">Hauptversion</sys:String>
	<sys:String x:Key="LOC_AU_Minor">Minor-Version</sys:String>
	<sys:String x:Key="LOC_AU_Build">Build-Version</sys:String>
    <sys:String x:Key="LOC_AU_MajorTooltip" xml:space="preserve">Die Hauptversion des Updates ist höher als die Hauptversion der installierten Erweiterung.
Z.B. von 1.2.1 auf 2.0.1, aber nicht von 1.2.1 auf 1.3.0.</sys:String>
    <sys:String x:Key="LOC_AU_MinorTooltip" xml:space="preserve">Die Minor-Version des Updates ist höher als die Minor-Version der installierten Erweiterung.
Z.B. von 1.2.1 auf 1.3.2, aber nicht von 1.2.1 auf 2.3.0.</sys:String>
    <sys:String x:Key="LOC_AU_BuildTooltip" xml:space="preserve">Die Build-Version des Updates ist höher als die Build-Version der installierten Erweiterung.
Z.B. von 1.2.1 auf 1.2.2, aber nicht von 1.2.1 auf 1.3.0.</sys:String>
</ResourceDictionary>
